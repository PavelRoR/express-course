@font-face {
	font-family: 'Akrobat';
	src: url('../fonts/Akrobat.eot');
	src: url('../fonts/Akrobat.eot') format('embedded-opentype'),
		 url('../fonts/Akrobat.woff2') format('woff2'),
		 url('../fonts/Akrobat.woff') format('woff'),
		 url('../fonts/Akrobat.ttf') format('truetype'),
		 url('../fonts/Akrobat.svg#Akrobat') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'AkrobatBold';
	src: url('../fonts/AkrobatBold.eot');
	src: url('../fonts/AkrobatBold.eot') format('embedded-opentype'),
		 url('../fonts/AkrobatBold.woff2') format('woff2'),
		 url('../fonts/AkrobatBold.woff') format('woff'),
		 url('../fonts/AkrobatBold.ttf') format('truetype'),
		 url('../fonts/AkrobatBold.svg#AkrobatBold') format('svg');
	font-display: fallback;
	font-weight: bold;
	font-style: normal;
}
@font-face {
	font-family: 'AkrobatExtraBold';
	src: url('../fonts/AkrobatExtraBold.eot');
	src: url('../fonts/AkrobatExtraBold.eot') format('embedded-opentype'),
		 url('../fonts/AkrobatExtraBold.woff2') format('woff2'),
		 url('../fonts/AkrobatExtraBold.woff') format('woff'),
		 url('../fonts/AkrobatExtraBold.ttf') format('truetype'),
		 url('../fonts/AkrobatExtraBold.svg#AkrobatExtraBold') format('svg');
	font-display: fallback;
	font-weight: 900;
	font-style: normal;
}
@font-face {
	font-family: 'Pattaya';
	src: url('../fonts/Pattaya.eot');
	src: url('../fonts/Pattaya.eot') format('embedded-opentype'),
		 url('../fonts/Pattaya.woff2') format('woff2'),
		 url('../fonts/Pattaya.woff') format('woff'),
		 url('../fonts/Pattaya.ttf') format('truetype'),
		 url('../fonts/Pattaya.svg#Pattaya') format('svg');
	font-display: fallback;
	font-weight: bold;
	font-style: italic;
}


@import '../css/_css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	background-color #fff
	font-size: 18px
	font-family: "Akrobat", sans-serif
	line-height: 1.3
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video_wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.general_title
	font-size: 36px;
	font-family: "AkrobatBold", sans-serif;
	color: rgb(55, 122, 105);
	text-transform: uppercase;
	line-height: 1;
	text-align: center;
	margin-bottom: 50px
	span
		display: inline-block
		color #fedc18
		background-color: #377a69
		padding 0px 3px
.general_title_light
	color #ffffff
	span
		background-color: #f9dd1a
		color #377a69
.button
	display block
	margin 0 auto
	font-size: 36px;
	font-family: "Akrobat", sans-serif;
	text-align: center;
	border: 2px solid ;
	width: 306px;
	height: 50px;
	line-height: 42px;
	background-color: transparent
	text-decoration: none
	transition: all .2s ease
	&:focus
		text-decoration: none
.button_yellow
	color: rgb(254, 220, 24);
	border-color: rgb(254, 220, 24)
	&:focus
		color #007B68
	&:hover
		color: #007B68
		background-color #FFDE00
.button_green
	color #377a69
	border-color: #377a69
	&:focus
		color #377a69
	&:hover
		color: #377a69
		background-color #FFDE00

